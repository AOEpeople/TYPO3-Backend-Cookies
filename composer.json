{
    "name": "aoe/becookies",
    "description": "This extension allows to handle backend session cookies and sets them for view domains that are different to the backend domain",
    "type": "typo3-cms-extension",
    "homepage": "https://forge.typo3.org/projects/extension-becookies",
    "license": "GPL-3.0",
    "support": {
        "email": "dev@aoe.com",
        "issues": "https://forge.typo3.org/projects/extension-becookies/issues",
        "source": "https://forge.typo3.org/projects/extension-becookies"
    },
    "authors": [
        {
            "name": "AOE GmbH",
            "email": "dev@aoe.com",
            "homepage": "http://www.aoe.com"
        }
    ],
    "require": {
        "typo3/cms-core": "*"
    },
    "require-dev": {
        "typo3/cms": "~8.0",
        "phpunit/phpunit": "4.8.*",
        "phpunit/phpcov": "2.0.*",
        "squizlabs/php_codesniffer": "2.3.*",
        "sebastian/phpcpd": "2.0.*",
        "phpmd/phpmd": "2.2.*"
    },
    "replace": {
        "becookies": "*"
    },
    "keywords": [
        "TYPO3",
        "extbase",
        "becookies"
    ],
    "autoload": {
        "psr-4": {
            "AOE\\BeCookies\\": "Classes"
        }
    }
}
